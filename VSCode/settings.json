{
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.fontFamily": "PragmataPro Mono Liga",
  "editor.fontSize": 12,
  "editor.tabSize": 2,
  "editor.fontLigatures": true,
  "editor.formatOnSave": false,
  "editor.lineNumbers": "relative",
  "editor.lineHeight": 18,
  "editor.renderWhitespace": "all",
  "editor.renderLineHighlight": "all",
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.rulers": [80],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "docker.showExplorer": false,
  "workbench.colorTheme": "Gruvbox Dark Soft",
  "workbench.editor.showTabs": "multiple",
  "workbench.sideBar.location": "left",
  "workbench.panel.defaultLocation": "right",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontFamily": "PragmataPro Liga",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.scrollback": 10000,
  "python.pythonPath": "python3",
  "reason.codelens.enabled": true,
  "reason.diagnostics.tools": ["merlin", "bsb"],
  "reason.path.bsb": "node_modules/.bin/bsb",
  // "editor.tokenColorCustomizations": {
  //     // https://github.com/arcticicestudio/nord-vim/issues/48#issuecomment-330740663
  //     "textMateRules": [
  //         {
  //             "name": "Comment",
  //             "scope": "comment",
  //             "settings": {
  //                 "foreground": "#616e88"
  //             }
  //         }
  //     ]
  // },
  "org.todoKeywords": [
    "PLAN",
    "TODO",
    "NEXT",
    "DONE",
    "STALE",
    "CANCELLED",
    "BLOCKED"
  ],
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.statusBar.enabled": false,
  "vim.leader": "<Space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      // Split line and move the trailing part down
      "before": ["K"],
      "after": ["i", "<CR>", "<Esc>", "k", "$"]
    },
    {
      "before": ["<leader>", "v"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.smartSelect.grow"
        }
      ]
    },
    {
      "before": ["<leader>", "V"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.smartSelect.shrink"
        }
      ]
    },
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    }
  ],
  "git.confirmSync": false,
  "vim.disableExtension": false,
  "gitlens.keymap": "chorded",
  "[ruby]": {
    "editor.rulers": [80],
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "Shopify.ruby-lsp",
    "editor.insertSpaces": true,
    "files.trimTrailingWhitespace": true,
    "editor.semanticHighlighting.enabled": true
  },
  "[nix]": {
    "editor.rulers": [80]
  },
  "[markdown]": {
    "editor.wordWrap": "bounded",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.formatOnSave": false
  },
  "[plaintext]": {
    "editor.detectIndentation": false
  },
  "workbench.startupEditor": "none",
  "workbench.statusBar.feedback.visible": false,
  "diffEditor.ignoreTrimWhitespace": true,
  "workbench.iconTheme": null,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "prettier.bracketSpacing": false,
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  // "ruby.useLanguageServer": true,
  "ruby.intellisense": "rubyLocate",
  "elixirLS.fetchDeps": false,
  "files.insertFinalNewline": true,
  "reason.path.ocamlmerlin": "ocamlmerlin-server",
  "files.defaultLanguage": "markdown",
  "revealjs.highlightTheme": "Solarized Light",
  "terminal.integrated.cursorBlinking": true,
  "reason_language_server.autoRebuild": false,
  "workbench.enableExperiments": false,
  "typescript.disableAutomaticTypeAcquisition": true,
  "rust-client.rustupPath": "/Users/ryanartecona/.cargo/bin/rustup",
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?_",
  "gitlens.views.repositories.location": "scm",
  "gitlens.views.fileHistory.location": "explorer",
  "gitlens.views.lineHistory.location": "explorer",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "reason_language_server.per_value_codelens": true,
  "terminal.integrated.rendererType": "dom",
  "elixir.autoSpawnElixirSenseServers": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "python.terminal.activateEnvironment": false,
  "python.analysis.memory.keepLibraryLocalVariables": true,
  "python.autoComplete.addBrackets": true,
  "python.linting.mypyEnabled": true,
  "python.workspaceSymbols.enabled": true,
  "terraform.indexing": {
    "enabled": false,
    "liveIndexing": false,
    "delay": 500,
    "exclude": [
      ".terraform/**/*",
      "**/.terraform/**/*"
    ]
  },
  "git.githubAuthentication": false,
  "python.languageServer": "Pylance",
  "python.showStartPage": false,
  "python.analysis.completeFunctionParens": true,
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb",
    "git-rebase-todo": "default"
  },
  "editor.linkedEditing": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.multiCursorModifier": "ctrlCmd",
  "fish.path.fish": "",
  "fish.path.fish_indent": "/Users/rartecona/.nix-profile/bin/fish_indent",
  // VS Code doesn't seem to respect OS user default shell >:|
  "terminal.integrated.defaultProfile.osx": "fish",
  "editor.guides.indentation": false,
  "python.testing.pytestEnabled": true,
  "python.venvPath": "~/.local/share/virtualenvs",
  "redhat.telemetry.enabled": false,
  "terraform.languageServer.enable": true,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "gitlens.advanced.fileHistoryFollowsRenames": true,
  "autoAlign.associations": {
    "csv": ",",
    "bsv": "|",
    "hcl": " =",
    "tf": " ="
  },
  "rubyLsp.rubyVersionManager": "rbenv",
  "go.toolsManagement.autoUpdate": true,
  "workbench.editor.empty.hint": "hidden",
  "terminal.integrated.tabs.enabled": false,
  "typescript.tsserver.maxTsServerMemory": 6144,
  "window.zoomLevel": 1,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.autoFormatStrings": true,
  "workbench.activityBar.location": "hidden",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "vscode-neovim.editorLangIdExclusions": [
    "magit"
  ],
  "workbench.editor.empty.hint": "hidden",
  // "editor.quickSuggestions": null,
}
